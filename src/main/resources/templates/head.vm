    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>${title}</title>
    <meta name="description" content="${description}" />
    <meta name="author" content="Rachel Diesel &amp; Bryan J Swift" />
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=0" />
    #if ($ogTitle && $ogDescription)
    <!-- Facebook stuff -->
    <meta property="og:title" content="${ogTitle}" />
    <meta property="og:description" content="${ogDescription}" />
    #end
    <meta property="og:image" content="http://persnicketly.com/img/open-graph-icon.png" />
    <meta property="og:site_name" content="Persnicketly" />
    <meta property="og:type" content="persnicketly:webapp" />
    <meta property="fb:admins" content="678400723" />
    <!-- Favicons -->
    <link rel="apple-touch-icon" href="apple-touch-icon.png" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <link href="/humans.txt" rel="author" />
    <!-- Includes -->
    #if ($css)
    <link href="${css}" rel="stylesheet" type="text/css" />
    #else
    <link href="/css/persnicketly.css" rel="stylesheet" type="text/css" />
    #end
    <script src="/js/libs/modernizr-custom.js"></script>
    <script src="http://use.typekit.com/rxz7gnq.js"></script>
    <script>
    (function(d,t) {
      try{Typekit.load();}catch(e){}
      var isLive = (location.host || location.hostname).match(/.*persnicket(ly|lyapp|yapp).com/i)
        , el = d.createElement(t)
        , sib = d.getElementsByTagName(t)[0];
      el.src = '//mint.persnicketly.com/?js';
      el.async = 1;
      if (isLive) { sib.parentNode.insertBefore(el, sib); }
    })(document, 'script');
    </script>

